{
  "name": "lazy-project",
  "version": "0.0.0-development",
  "description": "This is a project to show the lazy loading methods.",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "start": "webpack-dev-server -r dotenv/config",
    "build:dev": "npm run clean && DOTENV_CONFIG_PATH=./.env webpack -r dotenv/config",
    "build:prod": "npm run clean && DOTENV_CONFIG_PATH=./.env.production webpack -r dotenv/config",
    "test": "npm run test:eslint && npm run test:sass-lint && npm run test:flow && npm run test:jest",
    "test:eslint": "eslint src/**/*.{js,jsx}",
    "test:sass-lint": "sass-lint -v -q",
    "test:flow": "flow",
    "test:jest": "jest",
    "test:majestic": "majestic",
    "test:jest:watch": "jest --watch",
    "panic": "rimraf package-lock.json && rimraf node_modules && npm install",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "precommit": "npm test",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "codecov": "codecov && cat ./coverage/lcov.info | codacy-coverage",
    "travis-deploy-once": "travis-deploy-once"
  },
  "keywords": [
    "lazy-loading"
  ],
  "author": "Gustavo Mu√±oz <timbergus@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@emotion/core": "^10.0.15",
    "@loadable/component": "^5.10.2",
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.0",
    "apollo-boost": "^0.4.4",
    "autoprefixer": "^9.6.1",
    "babel-plugin-universal-import": "^4.0.0",
    "codacy-coverage": "^3.4.0",
    "dotenv": "^8.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "html-webpack-plugin": "^3.2.0",
    "majestic": "^1.6.0",
    "mini-css-extract-plugin": "^0.8.0",
    "react": "^16.9.0",
    "react-apollo": "^3.0.1",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.11",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.0.1",
    "react-spinners": "^0.6.0",
    "react-universal-component": "^4.0.0",
    "styled-components": "^4.3.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "codecov": "^3.5.0",
    "commitizen": "^4.0.3",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "docz": "1.2.0",
    "docz-theme-default": "1.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^5.0.0",
    "flow-bin": "^0.106.0",
    "graphql-import-loader": "^0.2.1",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.0",
    "semantic-release": "^15.13.21",
    "semantic-release-cli": "^5.1.2",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timbergus/lazy-project.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12.8.0",
    "npm": ">=6.10.0"
  }
}
